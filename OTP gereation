package Method;

public class task {
public static void details(String name,String job,String salary) {
	
	System.out.println("Name"+" "+name);
	System.out.println("job"+" "+job);
	System.out.println("Salary"+" "+salary);
	System.out.println("Name"+" "+name);
	System.out.println("job"+" "+job);
	System.out.println("Salary"+" "+salary);
}
	public static void main(String args[]) {
		details("Keerthana","ADMIN","10000");
		details("Swetha","Developer","20000");
		
		}
}
