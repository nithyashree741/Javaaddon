package constructor;

public class Laptop {
	String model;
	int size;
	public Laptop(String model,int size){
		this.model=model;
		this.size=size;;
	}
	void display(){
		System.out.println("LAPTOP MODEL"+ model +" "+"and ram size is"+" "+size);
	}
	public static void main(String args[]){
		Laptop l=new Laptop("dell",16);
		l.display();
	}

}
