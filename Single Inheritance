package Inheritance;

public class Singlelevel {
	void program(){
 System.out.println("Single level inheritance");
}
}
class subclass extends Singlelevel{
	void view(){
		System.out.println("Program Checking");
	}
}
class main{
	public static void main(String ar[]){
		subclass re=new subclass();
		re.program();
		re.view();
	}
}
